{
  "name": "chat-bot",
  "version": "1.0.0",
  "description": "chat bot built on dialogFlow",
  "main": "index.js",
  "engines": {
    "node": "14.15.4",
    "npm": "7.15.0"
  },
  "scripts": {
    "server": "nodemon backend/index.js",
    "client": "npm run start --prefix client",
    "start": "node backend/index.js",
    "dev": "concurrently \"npm run server\"  \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devAbhimanyu/chatbot-dialogFlow.git"
  },
  "author": "Manyu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devAbhimanyu/chatbot-dialogFlow/issues"
  },
  "homepage": "https://github.com/devAbhimanyu/chatbot-dialogFlow#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dialogflow": "^1.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@google-cloud/storage": "^5.14.0",
    "concurrently": "^6.2.1",
    "git-cz": "^4.7.6",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "backend/*.{js,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
